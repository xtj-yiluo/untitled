<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- 为javaBean类起别名 -->
    <typeAliases>
        <package name="com.sogaa.system.dao.mapper"/>
    </typeAliases>
    <!-- 通过mapper接口加载整个包的映射文件 -->
    <mappers>
        <package name="src/main/resources/mappers/personMapper"/>
    </mappers>

</configuration>
